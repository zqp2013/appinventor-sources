<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generatedFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generatedKeys="com.google.gwt.i18n.server.keygen.MethodNameKeyGenerator"
             ui:generateLocales="default">
  <ui:with field="messages" type="com.google.appinventor.client.OdeMessages" />
  <g:FlowPanel styleName="ode-StatusPanel" ui:field="footer">
    <g:HTMLPanel>
        <g:Anchor text="主题：" styleName="colorThemeTip"/>
        <li ui:field="classic" class="colorTheme classic" title="经典绿（原版）">
        </li>
        <li ui:field="lightgreen" class="colorTheme lightgreen" title="翡翠绿">
        </li>
        <li ui:field="gray" class="colorTheme gray" title="高级灰">
        </li>
        <li ui:field="blue" class="colorTheme blue" title="专业蓝">
        </li>
        <li ui:field="red" class="colorTheme red" title="中国红">
        </li>
        <li ui:field="pink" class="colorTheme pink" title="翘红">
        </li>
        <li ui:field="orange" class="colorTheme orange" title="活力橙">
        </li>
        
        <g:HTML ui:field="switchDark"></g:HTML>
    </g:HTMLPanel>
    <g:Anchor text="{messages.privacyTermsLink}" target="_blank" ui:field="tosLink" styleName="tosLink" />
    <g:Frame ui:field="searchFrame" styleName="searchFrame" />
  </g:FlowPanel>
</ui:UiBinder>
